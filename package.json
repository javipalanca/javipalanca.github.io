{
  "name": "spade-github-pages",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development astro dev --host",
    "start": "cross-env NODE_ENV=development astro dev --host",
    "build": "cross-env NODE_ENV=production astro build",
    "preview": "cross-env NODE_ENV=production astro preview --host",
    "astro": "astro",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "astro check",
    "manage": "python3 spade_manager.py",
    "news": "python3 manage_news.py",
    "demos": "python3 manage_demos.py",
    "backup": "python3 backup_data.py"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "astro": "^5.8.0",
    "typescript": "^5.8.3",
    "vis-network": "^9.1.10"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}